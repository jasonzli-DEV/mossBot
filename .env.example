# Discord Bot Token - Get from https://discord.com/developers/applications
BotToken=your_bot_token_here

# MongoDB Connection String - Get from MongoDB Atlas or your MongoDB instance
DatabaseURL=mongodb://localhost:27017/mossbot

# Activity Tracker - Channel ID where activity dashboard will be posted
ActivityTrackerChannelID=

# Welcome Messages - Channel ID for welcome messages when new members join
WelcomeChannelID=

# Music System - Voice channel ID where music will auto-play
MusicChannelID=

# Ticket System - Text channel ID where ticket panel will be posted
ticketChannelID=

# Minecraft Server Status - IP address of Minecraft server to monitor
minecraftServerIP=play.example.net

# Server Status Dashboard - Channel ID where server status will be posted
minecraftServerStatusChannelID=
